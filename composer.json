{
  "name": "gigabait93/filament-pages",
  "description": "Filamen custom pages plugin. Documentation: https://github.com/GIGABAIT93/filament-pages/tree/main/docs",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "GIGABAIT93",
      "email": "xgigabaitx@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^12.0",
    "filament/filament": "^4.0",
    "filament/forms": "^4.0",
    "filament/tables": "^4.0",
    "filament/actions": "^4.0",
    "filament/notifications": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Gigabait93\\FilamentPages\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gigabait93\\FilamentPages\\Providers\\FilamentPagesServiceProvider"
      ]
    }
  },
  "archive": {
    "exclude": ["docs"]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}